		{
			"sound": "dialogue/BFText",
			"speed": 0.05,
			"portrait": "bf",
			"boxState": "normal",
			"expression": "talk",
			"text": "And this girl is Barbara"
		},
		{
			"sound": "dialogue/BFText",
			"speed": 0.05,
			"portrait": "bf",
			"boxState": "normal",
			"expression": "talk",
			"text": "Sans, Barbara, this is Monika, the president of the Doki Doki Literature Club"
