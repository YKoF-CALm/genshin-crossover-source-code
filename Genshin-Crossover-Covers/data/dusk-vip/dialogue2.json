			"speed": 0.05,
			"sound": "dialogue/dialganyu"
		},
		{
			"expression": "talk",
			"text": "Hey, I think she's broken free!",
			"boxState": "normal",
			"portrait": "bf",
			"speed": 0.05,
			"sound": "dialogue/BFText"
		},
		{
